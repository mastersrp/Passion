<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="interpreter" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Option virtualFolders="src/;" />
		<Build>
			<Target title="Debug">
				<Option output="../bin/interpreter-d" prefix_auto="1" extension_auto="1" />
				<Option working_dir="../bin" />
				<Option object_output="../../temp" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
					<Add option="-D_DEBUG_" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output="../bin/interpreter" prefix_auto="1" extension_auto="1" />
				<Option working_dir="../bin" />
				<Option object_output="../../temp" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-fexceptions" />
			<Add option="-DPASSION_PLATFORM_LINUX" />
			<Add directory="../../include" />
		</Compiler>
		<Linker>
			<Add library="libdl" />
		</Linker>
		<Unit filename="../interpreter/Interpreter.cpp">
			<Option virtualFolder="src/" />
		</Unit>
		<Extensions>
			<code_completion />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
